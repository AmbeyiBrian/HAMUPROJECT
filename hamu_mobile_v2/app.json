{
  "expo": {
    "name": "HAMU Water",
    "slug": "hamu-water",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0077B6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hamu.water"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0077B6"
      },
      "package": "com.hamu.water"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "@react-native-community/datetimepicker"
    ],
    "scheme": "hamu",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2c013a5a-c18c-4c3e-b4a9-04693093c7bd"
      }
    }
  }
}
